{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "mongoose",
      "settings": {
        "host": "${process.env.HOST || '0.0.0.0'}",
        "port": "${process.env.PORT || 1337}",
        "uri": "${process.env.DATABASE_URI || ''}",
        "database": "cmDB"
      },
      "options": {
        "ssl": true
      }
    }
  }
}
